# use module
use strict;
use warnings;
use XML::Simple;
use Data::Dumper;
use HTML::Parser;
use Text::CSV;

# create object
my $xml = new XML::Simple (KeyAttr=>[]);

# read XML file and store to array
my $smap = $xml->XMLin("sitemap.xml");

# iterate through that array, pull out the 'loc' element and print it
foreach $e (@{$smap->{url}})
{
	my $p = new HTML::Parser;
	$p->parse_file("$e->{loc}");
}
